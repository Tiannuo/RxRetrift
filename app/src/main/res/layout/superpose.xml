<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:id="@+id/ll_contain">
    <ImageView
        android:id="@+id/iv_seek"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/umeng_socialize_sina"
        android:background="#33444444"
        />

    <ImageView
        android:id="@+id/iv_seek2"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="90dp"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/umeng_socialize_qq"
        android:background="#33EE446c"
        />

    <TextView
        android:layout_gravity="bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_seek"
        />


</FrameLayout>